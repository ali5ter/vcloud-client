                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Features" type="NetworkFeaturesType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Network features like DHCP, firewall and NAT rules.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SyslogServerSettings" type="SyslogServerSettingsType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Syslog server settings for the network.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="RouterInfo" type="RouterInfoType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Specifies router information.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

