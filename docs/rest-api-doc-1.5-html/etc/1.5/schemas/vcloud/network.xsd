<xs:schema xmlns="http://www.vmware.com/vcloud/v1.5" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.vmware.com/vcloud/v1.5" version="1.0">

    <xs:include schemaLocation="common.xsd"/>
    <xs:include schemaLocation="entity.xsd"/>

    <xs:annotation>
        <xs:documentation xml:lang="en">
            Schema for the vCloud Network types.
        </xs:documentation>
    </xs:annotation>

    <xs:complexType abstract="true" name="NetworkType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a Network in the vCloud model.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="EntityType">
                <xs:sequence>
                    <xs:element name="Configuration" type="NetworkConfigurationType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Network configuration.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="OrgNetworkType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents an organization network in the vCloud model.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="NetworkType">
                <xs:sequence>
                    <xs:element name="NetworkPool" type="ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Network Pool from which this organization network is created.
                                Not present if it is bridged to an external network.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="AllowedExternalIpAddresses" type="IpAddressesType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Optional set of IP addresses that this organization network is
                                limited to using for purposes of NAT.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VAppNetwork" type="VAppNetworkType"/>
    <xs:complexType name="VAppNetworkType">
        <xs:annotation>
            <xs:documentation source="since">1.5</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a vApp network.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="NetworkType">
                <xs:attribute name="deployed" type="xs:boolean" use="optional">
                    <xs:annotation>
                        <xs:documentation source="since">1.5</xs:documentation>
                        <xs:documentation source="modifiable">none</xs:documentation>
                        <xs:documentation xml:lang="en">
                            True if the network is deployed.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="NetworkService" type="NetworkServiceType">
        <xs:annotation>
            <xs:documentation>
               This is the root of the substitution group for network
               services. Replace it with any of DhcpService, NatService,
               IpsecVpnService, DhcpService, or StaticRoutingService.
           </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="NetworkServiceType">
        <xs:annotation>
            <xs:documentation>This is the root type of all NetworkServices</xs:documentation>
        </xs:annotation>
       <xs:complexContent>
                <xs:extension base="VCloudExtensibleType">
                    <xs:sequence>
                        <xs:element name="IsEnabled" type="xs:boolean" minOccurs="0" maxOccurs="1">
                            <xs:annotation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Enable or disable the service using this flag
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
               </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="DhcpService" type="DhcpServiceType" substitutionGroup="NetworkService">
        <xs:annotation>
            <xs:documentation source="modifiable">always</xs:documentation>
            <xs:documentation xml:lang="en">
                DHCP service settings
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="FirewallService" type="FirewallServiceType" substitutionGroup="NetworkService">
        <xs:annotation>
            <xs:documentation source="modifiable">always</xs:documentation>
            <xs:documentation xml:lang="en">
                Firewall service settings
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="NatService" type="NatServiceType" substitutionGroup="NetworkService">
        <xs:annotation>
            <xs:documentation source="modifiable">always</xs:documentation>
            <xs:documentation xml:lang="en">
                NAT service settings
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="IpsecVpnService" type="IpsecVpnServiceType" substitutionGroup="NetworkService">
        <xs:annotation>
            <xs:documentation source="modifiable">always</xs:documentation>
            <xs:documentation xml:lang="en">
                Ipsec Vpn service settings
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="StaticRoutingService" type="StaticRoutingServiceType" substitutionGroup="NetworkService">
        <xs:annotation>
            <xs:documentation source="modifiable">always</xs:documentation>
            <xs:documentation xml:lang="en">
                Static Routing service settings
            </xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:complexType name="NetworkFeaturesType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Represents features of a network.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="NetworkService" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="RouterInfoType">
        <xs:sequence>
            <xs:element name="ExternalIp" type="IpAddressType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation source="modifiable">none</xs:documentation>
                    <xs:documentation xml:lang="en">
                        Specifies the external IP of the router. Applicable for NAT Routed / Fenced networks only.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="DhcpServiceType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Represents a DHCP network service.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="NetworkServiceType">
                <xs:sequence>
                    <xs:element name="DefaultLeaseTime" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Default lease in seconds for DHCP addresses.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="MaxLeaseTime" type="xs:int" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Max lease in seconds for DHCP addresses.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="IpRange" type="IpRangeType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                IP range for DHCP addresses.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="NatServiceType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Represents a NAT network service.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="NetworkServiceType">
                <xs:sequence>
                    <xs:element name="NatType" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                One of:
                                ipTranslation (use IP translation),
                                portForwarding (use port forwarding)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Policy" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                One of:
                                allowTraffic (Allow all traffic),
                                allowTrafficIn (Allow inbound traffic only)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NatRule" type="NatRuleType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A NAT rule.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="NatRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a NAT rule.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Description of NAT rule.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:choice>
                        <xs:element name="OneToOneBasicRule" type="NatOneToOneBasicRuleType">
                            <xs:annotation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Maps one internal IP address to one external IP address.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="OneToOneVmRule" type="NatOneToOneVmRuleType">
                            <xs:annotation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Maps one VM NIC to one external IP addresses.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="PortForwardingRule" type="NatPortForwardingRuleType">
                            <xs:annotation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Port forwarding internal to external IP addresses.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="VmRule" type="NatVmRuleType">
                            <xs:annotation>
                                <xs:documentation source="modifiable">always</xs:documentation>
                                <xs:documentation xml:lang="en">
                                    Port forwarding VM NIC to external IP addresses.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="NatOneToOneBasicRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents the NAT basic rule for one to one mapping of internal
                and external IP addresses from a network.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="MappingMode" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                One of:
                                automatic (map IP addresses automatically),
                                manual (map IP addresses manually using
                                ExternalIpAddress and InternalIpAddress)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ExternalIpAddress" type="IpAddressType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                External IP address to map.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InternalIpAddress" type="IpAddressType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Internal IP address to map.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="NatOneToOneVmRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents the NAT rule for one to one mapping of VM NIC and
                external IP addresses from a network.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="MappingMode" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Mapping mode.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ExternalIpAddress" type="IpAddressType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                External IP address to map to VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedVmId" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                VAppScopedVmId of the VM to which this rule
                                applies.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmNicId" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                VM NIC ID to which this rule applies.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="NatPortForwardingRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents the NAT rule for port forwarding between internal
                IP/port and external IP/port.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="ExternalIpAddress" type="IpAddressType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                External IP address to forward to.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ExternalPort" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                External port to forward to.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InternalIpAddress" type="IpAddressType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Internal IP address to forward to.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InternalPort" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Internal port to forward to.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Protocol" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Protocol to forward. One of:
                                TCP (forward TCP packets),
                                UDP (forward UDP packets),
                                TCP_UDP (forward TCP and UDP packets).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="NatVmRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents the NAT rule for port forwarding between VM NIC/port
                and external IP/port.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="ExternalIpAddress" type="IpAddressType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                External IP address to forward to.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ExternalPort" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                External port to forward to.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedVmId" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                VAppScopedVmId of VM to which this rule
                                applies.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmNicId" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                VM NIC ID to which this rule applies.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InternalPort" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Internal port to forward.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Protocol" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Protocol to forward. One of:
                                TCP (forward TCP packets),
                                UDP (forward UDP packets),
                                TCP_UDP (forward TCP and UDP packets).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="FirewallServiceType">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Represents a network firewall service.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="NetworkServiceType">
                <xs:sequence>
                    <xs:element name="DefaultAction" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Default action of the firewall. One of:
                                drop (Default. Drop packets that match the
                                rule.),
                                allow (Allow packets that match the rule to pass
                                through the firewall)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="LogDefaultAction" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Flag to enable logging for default action. Default value is false.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="FirewallRule" type="FirewallRuleType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A firewall rule.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

     <xs:complexType name="FirewallRuleType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a firewall rule.
            </xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="IsEnabled" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Used to enable or disable the firewall rule. Default value is true.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A description of the rule.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Policy" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                One of:
                                drop (drop packets that match the rule),
                                allow (allow packets that match the rule to pass
                                through the firewall)
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Protocols" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Specify the protocols to which the rule should be applied.
                            </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:choice>
                                <xs:sequence>
                                    <xs:element maxOccurs="1" minOccurs="0" name="Tcp" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:documentation source="modifiable">always</xs:documentation>
                                            <xs:documentation xml:lang="en">
                                                True if the rule applies to the
                                                TCP protocol.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="0" name="Udp" type="xs:boolean">
                                        <xs:annotation>
                                            <xs:documentation source="modifiable">always</xs:documentation>
                                            <xs:documentation xml:lang="en">
                                                True if the rule applies to the
                                                UDP protocol.
                                            </xs:documentation>
                                        </xs:annotation>
                                    </xs:element>
                                </xs:sequence>
                                <xs:element maxOccurs="1" minOccurs="0" name="Icmp" type="xs:boolean">
                                     <xs:annotation>
                                         <xs:documentation source="modifiable">always</xs:documentation>
                                         <xs:documentation xml:lang="en">
                                                True if the rule applies to the
                                                ICMP protocol.
                                         </xs:documentation>
                                     </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="1" minOccurs="0" name="Any" type="xs:boolean">
                                     <xs:annotation>
                                         <xs:documentation source="modifiable">always</xs:documentation>
                                         <xs:documentation xml:lang="en">
                                                True if the rule applies to any
                                                protocol.
                                         </xs:documentation>
                                     </xs:annotation>
                                </xs:element>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="Port" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The port to which this rule applies. A value of
                                -1 matches any port.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DestinationIp" type="FirewallIpAddressType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Destination IP address to which the rule
                                applies. A value of Any matches any IP address.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SourcePort" type="xs:int" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Destination port to which this rule applies.
                                A value of -1 matches any port.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SourceIp" type="FirewallIpAddressType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Source IP address to which the rule applies.
                                A value of Any matches any IP address.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Direction" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Direction of traffic to which rule applies. One
                                of:
                                in (rule applies to incoming traffic. This is
                                the default value),
                                out (rule applies to outgoing traffic).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EnableLogging" type="xs:boolean" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
