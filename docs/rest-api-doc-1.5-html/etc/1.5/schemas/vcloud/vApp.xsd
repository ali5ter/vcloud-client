                    <xs:element minOccurs="0" ref="AllEULAsAccepted">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Used to confirm acceptance of all EULAs in a
                                vApp template. Instantiation fails if this
                                element is missing, empty, or set to false
                                and one or more EulaSection elements are
                                present.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="linkedClone" type="xs:boolean" use="optional">
