<xs:schema xmlns="http://www.vmware.com/vcloud/extension/v1.5" xmlns:vcloud="http://www.vmware.com/vcloud/v1.5" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.vmware.com/vcloud/extension/v1.5" version="1.0">

    <xs:import namespace="http://www.vmware.com/vcloud/v1.5" schemaLocation="../master/master.xsd"/>

    <xs:include schemaLocation="notification.xsd"/>
    <xs:include schemaLocation="settings.xsd"/>
    <xs:include schemaLocation="taskExtensionRequest.xsd"/>

    <xs:annotation>
        <xs:documentation xml:lang="en">
            Schema for VMware's specific extensions of the vCloud API
        </xs:documentation>
    </xs:annotation>

    <xs:element name="VMWExtension" type="VMWExtensionType"/>
    <xs:complexType name="VMWExtensionType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                List links to extension operations and entities.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ResourcePoolType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                vSphere resource pool information.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:EntityType">
                <xs:sequence>
                  <xs:element name="MoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The vSphere managed object reference of the
                                resource pool.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                  <xs:element name="VimObjectType" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The vCenter server that provides the resources
                                in this pool.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                  <xs:element name="DataStoreRefs" type="VimObjectRefsType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vSphere datastores accessible from this
                                resource pool.
                            </xs:documentation>
                        </xs:annotation>
                       </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="PrepareHostParams" type="PrepareHostParamsType"/>
    <xs:complexType name="PrepareHostParamsType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Parameters for preparing an ESX/ESXi host.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Username" type="xs:string">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        The user name to use for host preparation login.
                       </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Password" type="xs:string">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        The cleartext password to use for host preparation login.
                     </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ServerType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Information needed to connect to a vSphere or a vShield manager
                server.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:EntityType">
                <xs:sequence>
                    <xs:element name="Username" type="xs:string">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                User name to connect to the server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Password" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Cleartext password to connect to the server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Url" type="xs:anyURI">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                URL of the server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="RegisterVimServerParams" type="RegisterVimServerParamsType"/>
    <xs:complexType name="RegisterVimServerParamsType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to register a vCenter server.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VimServer" type="VimServerType">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        vCenter administrator credentials. Required for
                        register, modify.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShieldManager" type="ShieldManagerType">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        vShield manager administrator credentials. Required for
                        register, modify.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="VimServer" type="VimServerType"/>
    <xs:complexType name="VimServerType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents vCenter server information.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ServerType">
                <xs:sequence>
                    <xs:element name="IsEnabled" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                True if the vCenter server is enabled for use
                                with vCloud Director.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="IsConnected" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                True if the vCenter server is connected.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ShieldManagerHost" type="xs:anyURI" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vShield manager IP address or fully qualified
                                domain name.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ShieldManagerUserName" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vShield manager user name.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Uuid" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.5</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The instance UUID property of the vCenter server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VMWVimServerReferences" type="VMWVimServerReferencesType"/>
    <xs:complexType name="VMWVimServerReferencesType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                List of references to vCenter servers registered to vCloud
                Director.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="VimServerReference" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Reference to a vCenter server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ShieldManager" type="ShieldManagerType"/>
    <xs:complexType name="ShieldManagerType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents credentials to a vShield Manager server.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ServerType">
                <xs:sequence>
                    <xs:element name="AssociatedVimServer" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Reference to the vCenter server for this
                                vShield manager.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VmObjectRefsList" type="VmObjectRefsListType"/>
    <xs:complexType name="VmObjectRefsListType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                A list of references to virtual machines in vCenter inventory.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VmObjectRef" type="VmObjectRefType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation source="modifiable">none</xs:documentation>
                    <xs:documentation xml:lang="en">
                         A reference to a virtual machine in vCenter inventory.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="numberOfPages" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation source="modifiable">none</xs:documentation>
                <xs:documentation xml:lang="en">
                    The total number of pages in the response.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="page" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation source="modifiable">none</xs:documentation>
                <xs:documentation xml:lang="en">
                    The current page number. The first page is page number 1.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="VmObjectRefType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                A reference to a virtual machine in vCenter inventory.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VimObjectRefType">
                <xs:attribute name="name" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The name of the virtual machine.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportVmAsVAppParams" type="ImportVmAsVAppParamsType"/>
    <xs:complexType name="ImportVmAsVAppParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Parameters to import a virtual machine as a vApp.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:VAppCreationParamsType">
                <xs:sequence>
                    <xs:element name="VmName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedLocalId" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vApp scoped local id to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ComputerName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Computer name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmMoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Managed object reference of VM to import.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Vdc" type="vcloud:ReferenceType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Target vDC for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Set to true to delete the source object after the
                            import is complete.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportVmIntoExistingVAppParams" type="ImportVmIntoExistingVAppParamsType" />
    <xs:complexType name="ImportVmIntoExistingVAppParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.5
            </xs:documentation>
            <xs:documentation xml:lang="en">
                Parameters to import a virtual machine into an existing VApp.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:VCloudExtensibleType">
                <xs:sequence>
                    <xs:element name="VmName" type="xs:string"
                        minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                Name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmDescription" type="xs:string"
                        minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                Description to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedLocalId"
                        type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                vApp scoped local id to be used for imported
                                VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ComputerName" type="xs:string"
                        minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                Computer name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmMoRef" type="xs:string"
                        minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                 Managed object reference of VM to import.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VApp" type="vcloud:ReferenceType"
                        minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always
                            </xs:documentation>
                            <xs:documentation xml:lang="en">
                                vApp to hold imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean"
                    default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always
                        </xs:documentation>
                        <xs:documentation xml:lang="en">
                            Set to true to delete the source object after the
                            import is complete.
                </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportVmAsVAppTemplateParams" type="ImportVmAsVAppTemplateParamsType"/>
    <xs:complexType name="ImportVmAsVAppTemplateParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Parameters to import a virtual machine as a vApp template.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ParamsType">
                <xs:sequence>
                    <xs:element name="VmName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedLocalId" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vApp scoped local id to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ComputerName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Computer name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmMoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Managed object reference of VM to import.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Vdc" type="vcloud:ReferenceType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Target vDC for imported vApp template.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Catalog" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Catalog in which the imported vApp template
                                will be listed.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Set to true to delete the source object after the
                            import is complete.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute name="goldMaster" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation source="since">1.5</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Attribute to specify if template is gold master, the default is false.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportMediaParams" type="ImportMediaParamsType"/>
    <xs:complexType name="ImportMediaParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.5</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to import media from vSphere.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ParamsType">
                <xs:sequence>
                    <xs:element name="DatastoreMoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Managed object reference of the datastore from which media file will be imported.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SourcePath" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A full path to media file on the datastore.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Vdc" type="vcloud:ReferenceType" minOccurs="1" maxOccurs="1">
                         <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The vDC into which the media will be imported.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Catalog" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The catalog in which the imported media will be
                                listed.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="since">1.0</xs:documentation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Set to true to delete the source object after the
                            import is complete.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

