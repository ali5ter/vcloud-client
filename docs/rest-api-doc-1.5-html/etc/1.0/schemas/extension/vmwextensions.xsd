<xs:schema xmlns="http://www.vmware.com/vcloud/extension/v1" xmlns:vcloud="http://www.vmware.com/vcloud/v1" xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.vmware.com/vcloud/extension/v1" version="1.0">

    <xs:import namespace="http://www.vmware.com/vcloud/v1" schemaLocation="../master/master.xsd"/>

    <xs:annotation>
        <xs:documentation xml:lang="en">
            Schema for VMware's specific extensions of the vCloud API
        </xs:documentation>
    </xs:annotation>

    <xs:element name="VMWExtension" type="VMWExtensionType"/>
    <xs:complexType name="VMWExtensionType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                List links to extension operations and entities.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ResourcePoolType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Resource pool vSphere information.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:EntityType">
                <xs:sequence>
                  <xs:element name="MoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The moref of the resource pool.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                  <xs:element name="VimObjectType" type="VimObjectTypeEnum" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The vSphere server on which this resource pool is.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                  <xs:element name="DataStoreRefs" type="VimObjectRefsType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Contains data stores accessible from the resource pool.
                            </xs:documentation>
                        </xs:annotation>
                       </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="PrepareHostParams" type="PrepareHostParamsType"/>
    <xs:complexType name="PrepareHostParamsType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to prepare a host.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Username" type="xs:string">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        The user name to use for host preparation.
                        Required to set user name for prepare operation, on modify.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Password" type="xs:string">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        The plain password to use for host preparation.
                        Required to set user password for prepare operation, on modify.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ServerType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                General info to connect to a vSphere or a shield manager server.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:EntityType">
                <xs:sequence>
                    <xs:element name="Username" type="xs:string">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                User name to connect to the server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Password" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Plain text password to connect to the server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Url" type="xs:anyURI">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                URL of the server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="RegisterVimServerParams" type="RegisterVimServerParamsType"/>
    <xs:complexType name="RegisterVimServerParamsType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to register a vSphere server.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VimServer" type="VimServerType">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        The vSphere credentials.
                        Required to set vim server to register, on modify.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ShieldManager" type="ShieldManagerType">
                <xs:annotation>
                    <xs:documentation source="modifiable">always</xs:documentation>
                    <xs:documentation xml:lang="en">
                        The shield manager credentials.
                        Required to set shield manager, on modify.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="VimServer" type="VimServerType"/>
    <xs:complexType name="VimServerType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a vSphere server information.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ServerType">
                <xs:sequence>
                    <xs:element name="IsEnabled" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The enabled flag of vSphere.
                                Required to set enabled flag, on modify.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="IsConnected" type="xs:boolean" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Read only flag represents available connection to a vSphere server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ShieldManagerIP" type="xs:anyURI" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Shield manager IP.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ShieldManagerUserName" type="xs:string" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Shield manager user name.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VMWVimServerReferences" type="VMWVimServerReferencesType"/>
    <xs:complexType name="VMWVimServerReferencesType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a list of available vSphere servers.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ResourceType">
                <xs:sequence>
                    <xs:element name="VimServerReference" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Reference to a vSphere server.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ShieldManager" type="ShieldManagerType"/>
    <xs:complexType name="ShieldManagerType">
        <xs:annotation>
            <xs:documentation source="since">0.9</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents credentials to a shield manager server.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ServerType">
                <xs:sequence>
                    <xs:element name="AssociatedVimServer" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Reference to vSphere server of the shield manager.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="VmObjectRefsList" type="VmObjectRefsListType"/>
    <xs:complexType name="VmObjectRefsListType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents a list of VM vSphere object references.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VmObjectRef" type="VmObjectRefType" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation source="modifiable">none</xs:documentation>
                    <xs:documentation xml:lang="en">
                        A VM vSphere object reference.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="numberOfPages" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation source="modifiable">none</xs:documentation>
                <xs:documentation xml:lang="en">
                    The number of total pages in the list.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="page" type="xs:int" use="optional">
            <xs:annotation>
                <xs:documentation source="modifiable">none</xs:documentation>
                <xs:documentation xml:lang="en">
                    The current page number starting from 1.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="VmObjectRefType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents vSphere object information about a VM.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="VimObjectRefType">
                <xs:attribute name="name" type="xs:string" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            The name of VM.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportVmAsVAppParams" type="ImportVmAsVAppParamsType"/>
    <xs:complexType name="ImportVmAsVAppParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to import vSphere VM as VApp.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:VAppCreationParamsType">
                <xs:sequence>
                    <xs:element name="VmName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedLocalId" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vApp scoped local id to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ComputerName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Computer name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmMoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Required to set moref of VM which to import.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Vdc" type="vcloud:ReferenceType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Required to set in which vDC to import the VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Attribute to specify to move or not the source folder of the VM.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportVmAsVAppTemplateParams" type="ImportVmAsVAppTemplateParamsType"/>
    <xs:complexType name="ImportVmAsVAppTemplateParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to import vSphere VM as VApp template.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ParamsType">
                <xs:sequence>
                    <xs:element name="VmName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VAppScopedLocalId" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                vApp scoped local id to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ComputerName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Computer name to be used for imported VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="VmMoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Required to set moref of VM which to import.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Vdc" type="vcloud:ReferenceType" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Required to set in which vDC to import the VM.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Catalog" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Optional catalog reference to add to imported vApp template.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Attribute to specify to move or not the source folder of the VM.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="ImportMediaParams" type="ImportMediaParamsType"/>
    <xs:complexType name="ImportMediaParamsType">
        <xs:annotation>
            <xs:documentation source="since">1.0.1</xs:documentation>
            <xs:documentation xml:lang="en">
                Represents parameters to import media from vSphere.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:ParamsType">
                <xs:sequence>
                    <xs:element name="DatastoreMoRef" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Managed object reference of the datastore from which media file will be imported.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="SourcePath" type="xs:string" minOccurs="1" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                A full path to media file on the datastore.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Vdc" type="vcloud:ReferenceType" minOccurs="1" maxOccurs="1">
                         <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The vDC to which the media will be imported.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Catalog" type="vcloud:ReferenceType" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="since">1.0</xs:documentation>
                            <xs:documentation source="modifiable">always</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The catalog to which the media will be imported.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:attribute name="sourceMove" type="xs:boolean" default="false" use="optional">
                    <xs:annotation>
                        <xs:documentation source="since">1.0</xs:documentation>
                        <xs:documentation source="modifiable">always</xs:documentation>
                        <xs:documentation xml:lang="en">
                            Flag to remove the media from the vSphere after import.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:element name="Host" type="HostType"/>
    <xs:complexType name="HostType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Read only representation of one host in a vSphere server.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vcloud:EntityType">
                <xs:sequence>
                    <xs:element name="Ready" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Indicates if the host is ready for use.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Available" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Indicates if the host is available.
                                TODO what is difference between ready and available?
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Enabled" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Indicated if the host is enabled for use.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Busy" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Indicates if the host is busy.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EnableHostForHostSpanning" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                Indicates is the host enabled for host spanning.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CpuType" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The CPU type of the host.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NumOfCpusPackages" type="xs:int">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                How many hardware CPUs the host has.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NumOfCpusLogical" type="xs:int">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                How many logical CPUs the host has.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CpuTotal" type="xs:long">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The total CPU power measured in MHz.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="MemUsed" type="xs:double">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The used memory in the host.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="MemTotal" type="xs:double">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                The total memory in the host.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="HostOsName" type="xs:string" minOccurs="0" maxOccurs="1">
                        <xs:annotation>
                            <xs:documentation source="modifiable">none</xs:documentation>
                            <xs:documentation xml:lang="en">
                                OS name of the host.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
