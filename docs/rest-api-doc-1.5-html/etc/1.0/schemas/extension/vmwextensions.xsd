    <xs:element name="ManagedServer" type="LicensingManagedServerType"/>
    <xs:complexType name="LicensingManagedServerType">
        <xs:annotation>
            <xs:documentation source="since">1.0</xs:documentation>
            <xs:documentation xml:lang="en">
                Captures a single set of metrics for a managed server for a given sample.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VimObjectRef" type="VimObjectRefType" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation source="modifiable">none</xs:documentation>
                    <xs:documentation xml:lang="en">
                        Host moref in the vSphere.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="cpu" type="xs:int" use="required">
            <xs:annotation>
                <xs:documentation source="modifiable">none</xs:documentation>
                <xs:documentation xml:lang="en">
                    Number of CPUs in the server.
                </xs:documentation>
            </xs:annotation>
